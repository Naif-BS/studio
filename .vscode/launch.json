    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdbtarget",
            "request": "launch",
            "name": "Name here",
            "program": "${workspaceFolder}/${command:askProgramPath}"
        },
        {
            "type": "gdbtarget",
            "request": "attach",
            "name": "Name here",
            "program": "${workspaceFolder}/${command:askProgramPath}"
        },
        {
            "type": "gdb",
            "request": "launch",
            "name": "Name here",
            "program": "${workspaceFolder}/${command:askProgramPath}"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach",
            "program": "${workspaceFolder}/<program>"
        },


        {
            "type": "gdb",
            "request": "launch",
            "name": "gdb launch",
            "program": "${workspaceFolder}/${command:askProgramPath}"
        }
    ,
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Next.js",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "port": 9229,
            "autoAttachChildProcesses": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}